Option Explicit

Imports System
Imports System.IO
Imports System.Collections.Generic
Imports System.Data.Odbc

Dim objTrend As TrendCmdTarget
Dim objLabel As DrawCmdTarget
Dim iCounter As Integer
Dim nValueX() As Double
Dim nValueY() As Double
Dim nXLastPointCurve1 As Integer
Dim nYLastPointCurve1 As Integer
Dim NumMaxElements As Integer

Dim strWherePostazione As String

Const PennaX As String = "X"
Const PennaY As String = "Y"

Dim TornioOld As Integer = 0
Dim MarpossOld As Integer = 0
Dim LappatriceOld As Integer = 0
Dim LavatriceOld As Integer = 0
Dim CaricoOld As Integer = 0
Dim ScaricoOld As Integer = 0

Public Sub OnTimer()
	If spv_TrendPezziFiltro = 0 And [spv_RikLogTornio:NuovoRecord] &lt;&gt; TornioOld Then
		UpdateTrend()
		'[spv_RikLogTornio:NuovoRecord] = False
		TornioOld = [spv_RikLogTornio:NuovoRecord]
	End If
	If spv_TrendPezziFiltro = 1 And [spv_RikLogMarposs:NuovoRecord] &lt;&gt; MarpossOld Then
		UpdateTrend()
		'[spv_RikLogMarposs:NuovoRecord] = False
		MarpossOld = [spv_RikLogMarposs:NuovoRecord]
	End If
	If spv_TrendPezziFiltro = 2 And [spv_RikLogLappatrice:NuovoRecord] &lt;&gt; LappatriceOld Then
		UpdateTrend()
		'[spv_RikLogLappatrice:NuovoRecord] = False
		LappatriceOld = [spv_RikLogLappatrice:NuovoRecord]
	End If
	If spv_TrendPezziFiltro = 3 And [spv_RikLogLavatrice:NuovoRecord] &lt;&gt; LavatriceOld Then
		UpdateTrend()
		'[spv_RikLogLavatrice:NuovoRecord] = False
		LavatriceOld = [spv_RikLogLavatrice:NuovoRecord]
	End If
	If spv_TrendPezziFiltro = 4 And [spv_RikLogCarico:NuovoRecord] &lt;&gt; CaricoOld Then
		UpdateTrend()
		'[spv_RikLogCarico:NuovoRecord] = False
		CaricoOld = [spv_RikLogCarico:NuovoRecord]
	End If
	If spv_TrendPezziFiltro = 5 And [spv_RikLogScarico:NuovoRecord] &lt;&gt; ScaricoOld Then
		UpdateTrend()
		'[spv_RikLogScarico:NuovoRecord] = False
		ScaricoOld = [spv_RikLogScarico:NuovoRecord]
	End If

End Sub

Public Sub Click()
	UpdateTrend()
End Sub


Public Sub UpdateTrend()

	NumMaxElements = 100
	'Set Trend Object
	objTrend = GetSubObject("TrendProduzione").GetObjectInterface()
	objTrend.PenMaxValue(PennaX) =  NumMaxElements
	objTrend.PenMaxValue(PennaY) =  20
	objTrend.Samples = NumMaxElements

	'objLabel = GetSubObject("LabelTitolo").GetObjectInterface()


	'Set first value to zero to start from the axis origin
	SetVariableValue("spv_TrendX", 0)
	SetVariableValue("spv_TrendY", 0)

	nXLastPointCurve1 = 0
	nYLastPointCurve1 = 0

	'----------------------------
	'Fill the TrendXY Buffer Data
	ReDim nValueX(NumMaxElements-1)
	ReDim nValueY(NumMaxElements-1)

	'decodifico la condizione della select
	Select Case spv_TrendPezziFiltro
		Case 0
			strWherePostazione = "TORNIO"
			'objLabel.ObjectName = "Trend Tempo Permanenza Tornio"
		Case 1
			strWherePostazione = "MARPOSS"
			'objLabel.ObjectName = "Trend Tempo Permanenza Marposs"
		Case 2
			strWherePostazione = "LAPPATRICE"
			'objTrend.Name = "Trend Tempo Permanenza Lappatrice"
		Case 3
			strWherePostazione = "LAVATRICE"
			'objLabel.ObjectName = "Trend Tempo Permanenza Lavatrice"
		Case 4
			strWherePostazione = "CARICO"
			'objLabel.ObjectName = "Trend Tempo Permanenza Carico"
		Case 5
			strWherePostazione = "SCARICO"
			'objLabel.ObjectName = "Trend Tempo Permanenza Scarico"
		Case Else
			strWherePostazione = "%%"
			'objLabel.ObjectName = "Trend Tempo Permanenza Tutti Pezzi"
	End Select


	UpdateVariables("DSN=Cat11L6_c04016_DLR;Uid=supervisione;Pwd=supervisione;",nValueX,nValueY,NumMaxElements)


	'Set Trend Buffer Y Value
	objTrend.SetSamplesValue(PennaX, 0, CLng(NumMaxElements), nValueX)
	objTrend.SetSamplesValue(PennaY, 0, CLng(NumMaxElements), nValueY)


	'Refresh Trend Area
	objTrend.Requery
	objTrend.Refresh

	objTrend = Nothing
End Sub

Private Sub UpdateVariables( connectionString As String, ByRef xVal() As Double, ByRef yVal() As Double, maxElem As Integer )
		Dim vResult As Object
        Dim commandText As String
        Dim rCount As Integer
        Dim maxX As Integer

        Dim maxPermanenza, minPermanenza, permanenza As Integer

        If maxElem &lt;= 0 Then
			maxX = 100
		Else
			maxX = maxElem
		End If

		ReDim xVal(maxX-1)
		ReDim yVal(maxX-1)

		For rCount = 0 To maxX-1 Step 1
			xVal(rCount) = rCount
			yVal(rCount) = 0
		Next



		maxPermanenza = 0
		minPermanenza = 0
		Try
            Using connection As New System.Data.Odbc.OdbcConnection(connectionString)
                connection.Open()

			        commandText = "SELECT TOP 100 " &amp; _
                            " ROW_NUMBER() OVER(ORDER BY [LocalCol] DESC) As Row# " &amp; _
                            ",[TimeCol] " &amp; _
                            ",[MSecCol] " &amp; _
                            ",[LocalCol] " &amp; _
                            ",[UserCol] " &amp; _
                            ",[ReasonCol] " &amp; _
                            ",[CodicePezzo] " &amp; _
			        		",[Postazione] " &amp; _
                            ",[DataPrelievo] " &amp; _
                            ",[DataDeposito] " &amp; _
                            ",[Permanenza] " &amp; _
                      " FROM [supervisionec4016_cat11l6].[dbo].[LogPezzi] " &amp; _
			          " WHERE [Postazione] like '%" &amp; strWherePostazione &amp; "%' " &amp; _
                      " order by [LocalCol] desc;"
			        Debug.Print commandText

                    Using cmd As New System.Data.Odbc.OdbcCommand(commandText, connection)

                        Using reader As System.Data.Odbc.OdbcDataReader = cmd.ExecuteReader()

                            If reader.HasRows Then
                            	rCount = 0
                                While reader.Read()
                                    'xVal(rCount) =  reader(0)
                                    permanenza =  reader(10)
                                    xVal(rCount) = rCount
                                    'yVal(rCount) =  reader(10)
                                    yVal(rCount) =  permanenza
                                    rCount = rCount + 1
									maxPermanenza = IIf(permanenza &gt; maxPermanenza, permanenza, maxPermanenza)
									minPermanenza = IIf(permanenza &lt; minPermanenza, permanenza, minPermanenza)
                                End While
                            End If
                        End Using
                    End Using

            End Using
        Catch e As Exception
            MsgBox("Impossibile accedere al Database. Verificare che la connessione ODBC sia configurata correttamente. " &amp; _
                           vbCrLf &amp; "Eccezione: " &amp; e.Message &amp; ".")
            Exit Sub
        End Try

End Sub

Public Sub SynopticLoading() Handles .SynopticLoading
	spv_LogPezziFiltroPostazione = 0
End Sub
