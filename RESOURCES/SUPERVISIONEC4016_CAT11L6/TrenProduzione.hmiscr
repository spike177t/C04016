<ScriptCode StartSel="0" SelLength="0" OutStatusBar="1" OutLog="1" OutPrinter="1">'#Reference #System.Data, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089, processorArchitecture=x86
'Sostituita la Variabile 'spv_TornioY' con il nuovo nome 'spv_TrendY'
'Sostituita la Variabile 'spv_TornioX' con il nuovo nome 'spv_TrendX'
'#Language "WWB.NET"

Option Explicit

Imports System
Imports System.IO
Imports System.Collections.Generic
Imports System.Data.Odbc

Dim objTrend As TrendCmdTarget
Dim iCounter As Integer
Dim nValueX() As Double
Dim nValueY() As Double
Dim nXLastPointCurve1 As Integer
Dim nYLastPointCurve1 As Integer
'Dim nXLastPointCurve2 As Integer
'Dim nYLastPointCurve2 As Integer
Dim NumMaxElements As Integer

Public Sub Click()

	NumMaxElements = 100


	'Set Trend Object
	 objTrend = GetSynopticObject().GetSubObject("TrendProduzione").GetObjectInterface()
	objTrend.PenMaxValue("TornioX") =  NumMaxElements
	objTrend.PenMaxValue("TornioY") =  20
	'objTrend.PenMaxValue("VarX1") =  NumMaxElements * NumStepCurve2
	'objTrend.PenMaxValue("VarY1") =  NumMaxElementsCurve2
	objTrend.Samples = NumMaxElements

	'Set first value to zero to start from the axis origin
	SetVariableValue("spv_TrendX", 0)
	SetVariableValue("spv_TrendY", 0)

	nXLastPointCurve1 = 0
	nYLastPointCurve1 = 0

	'----------------------------
	'Fill the TrendXY Buffer Data
	ReDim nValueX(NumMaxElements-1)
	ReDim nValueY(NumMaxElements-1)

	UpdateVariables("DSN=Cat11L6_c04016_DLR;Uid=supervisione;Pwd=supervisione;",nValueX,nValueY,NumMaxElements)


	'Set Trend Buffer Y Value
	objTrend.SetSamplesValue("TornioX", 0, CLng(NumMaxElements), nValueX)
	objTrend.SetSamplesValue("TornioY", 0, CLng(NumMaxElements), nValueY)


	'Refresh Trend Area
	objTrend.Requery
	objTrend.Refresh

	objTrend = Nothing
End Sub

Private Sub UpdateVariables( connectionString As String, ByRef xVal() As Double, ByRef yVal() As Double, maxElem As Integer )
		Dim vResult As Object
        Dim commandText As String
        Dim rCount As Integer
        Dim maxX As Integer

        If maxElem &lt;= 0 Then
			maxX = 100
		Else
			maxX = maxElem
		End If

		ReDim xVal(maxX-1)
		ReDim yVal(maxX-1)

		For rCount = 0 To maxX-1 Step 1
			xVal(rCount) = 0
			yVal(rCount) = 0
		Next


		Try
            Using connection As New System.Data.Odbc.OdbcConnection(connectionString)
                connection.Open()

			        commandText = "SELECT TOP 100 " &amp; _
                            " ROW_NUMBER() OVER(ORDER BY [LocalCol] DESC) As Row# " &amp; _
                            ",[TimeCol] " &amp; _
                            ",[MSecCol] " &amp; _
                            ",[LocalCol] " &amp; _
                            ",[UserCol] " &amp; _
                            ",[ReasonCol] " &amp; _
                            ",[CodicePezzo] " &amp; _
			        		",[Postazione] " &amp; _
                            ",[DataPrelievo] " &amp; _
                            ",[DataDeposito] " &amp; _
                            ",[Permanenza] " &amp; _
                      " FROM [supervisionec4016_cat11l6].[dbo].[LogPezzi] " &amp; _
                      " order by [LocalCol] desc;"
			        Debug.Print commandText

                    Using cmd As New System.Data.Odbc.OdbcCommand(commandText, connection)

                        Using reader As System.Data.Odbc.OdbcDataReader = cmd.ExecuteReader()

                            If reader.HasRows Then
                            	rCount = 0
                                While reader.Read()
                                    'xVal(rCount) =  reader(0)
                                    xVal(rCount) = rCount
                                    yVal(rCount) =  reader(10)
                                    rCount = rCount + 1
                                End While
                            End If
                        End Using
                    End Using

            End Using
        Catch e As Exception
            MsgBox("Impossibile accedere al Database. Verificare che la connessione ODBC sia configurata correttamente. " &amp; _
                           vbCrLf &amp; "Eccezione: " &amp; e.Message &amp; ".")
            Exit Sub
        End Try

End Sub
